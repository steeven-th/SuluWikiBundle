{
  "name": "itech-world/sulu-wiki-bundle",
  "description": "SuluWikiBundle extends the Sulu CMS to offer wiki and documentation management features similar to WikiJS",
  "type": "sulu-bundle",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "sulu",
    "symfony",
    "symfony-ux",
    "cms",
    "content-management-system",
    "php",
    "wiki",
    "documentation"
  ],
  "authors": [
      {
      "name": "Steeven THOMAS",
      "homepage": "https://github.com/steeven-th"
      }
  ],
  "require": {
      "php": "^8.2",
      "sulu/sulu": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "symfony/debug-bundle": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/error-handler": "^6.4",
    "symfony/phpunit-bridge": "^6.4",
    "symfony/var-dumper": "^6.4",
    "symfony/web-profiler-bundle": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "ItechWorld\\SuluWikiBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ItechWorld\\SuluWikiBundle\\": "src/tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit"
    ],
    "phpunit": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../../assets/admin/node_modules",
      "options": {
        "symlink": true
      }
    }
  ]
}
